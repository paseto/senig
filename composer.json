{
    "name": "paseto/senig",
    "description": "Api for Web Services Senig",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Giovani Paseto",
            "email": "giovaniw2@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "zendframework/zend-soap": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Paseto\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpstan/phpstan": "^0.11.1"
    },
    "scripts": {
        "phpcbf": "vendor/bin/phpcbf --standard=psr2 src; vendor/bin/phpcbf --standard=psr2 tests",
        "phpcs": "vendor/bin/phpcs --standard=psr2 src; vendor/bin/phpcs --standard=psr2 tests",
        "phpstan": "vendor/bin/phpstan analyse --level=7 src/"
    }
}
